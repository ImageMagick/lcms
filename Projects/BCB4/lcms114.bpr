VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = "..\..\lib\bc\lcms114.lib"
OBJFILES = lcms114.obj ..\..\src\cmsxform.obj ..\..\src\cmscnvrt.obj ..\..\src\cmserr.obj \
  ..\..\src\cmsgamma.obj ..\..\src\cmsgmt.obj ..\..\src\cmsintrp.obj \
  ..\..\src\cmsio1.obj ..\..\src\cmslut.obj ..\..\src\cmsmatsh.obj \
  ..\..\src\cmsmtrx.obj ..\..\src\cmspack.obj ..\..\src\cmspcs.obj \
  ..\..\src\cmssamp.obj ..\..\src\cmswtpnt.obj ..\..\src\cmscam97.obj \
  ..\..\src\cmscam02.obj ..\..\src\cmsps2.obj ..\..\src\cmsnamed.obj \
  ..\..\src\cmsvirt.obj ..\..\src\cmscgats.obj
DEFFILE =
LIBFILES =
RESDEPEN = $(RESFILES)
RESFILES =
LIBRARIES =
PACKAGES =
# ---------------------------------------------------------------------------
PATHCPP = .;..\..\src
PATHASM = .;
PATHPAS = .;
PATHRC = .;
USERDEFINES =
SYSDEFINES =NO_STRICT
# ---------------------------------------------------------------------------
CFLAG1 = -I..\..\src;..\..\include;$(BCB)\include;$(BCB)\include\vcl -Od -Hc \
  -H=$(BCB)\lib\vcl40.csm -w -Ve -x- -RT- -r- -a8 -5 -d -k -y -v -vi- -c -b- \
  -w-par -w-inl -Vx -tWM -D$(SYSDEFINES);$(USERDEFINES)
PFLAGS = -U..\..\src;$(DEBUGLIBPATH) \
  -I..\..\src;..\..\include;$(BCB)\include;$(BCB)\include\vcl -$Y+ -$W -$O- -v \
  -JPHN -M
AFLAGS = /i..\..\src /i..\..\include /i$(BCB)\include /i$(BCB)\include\vcl /mu /w2 /zi
RFLAGS = -i..\..\src;..\..\include;$(BCB)\include;$(BCB)\include\vcl
LFLAGS = /E /C
# ---------------------------------------------------------------------------
ALLOBJ = $(OBJFILES)
